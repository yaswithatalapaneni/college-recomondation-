<?xml version="1.0" encoding="UTF-8"?>
<web-app>
    <servlet>
        <servlet-name>LoginD</servlet-name>
        <servlet-class>LoginD</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginD</servlet-name>
        <url-pattern>/LoginD</url-pattern> 
    </servlet-mapping>

    <servlet>
        <servlet-name>RegistrationServlet</servlet-name>
        <servlet-class>RegistrationServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegistrationServlet</servlet-name>
        <url-pattern>/register</url-pattern> 
    </servlet-mapping>


    <servlet>
        <servlet-name>ApplicationStatusServlet</servlet-name>
        <servlet-class>ApplicationStatusServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ApplicationStatusServlet</servlet-name>
        <url-pattern>/application</url-pattern> 
    </servlet-mapping>
    
    <servlet>
    	<servlet-name>finder</servlet-name>
    	<servlet-class>finder</servlet-class>
    </servlet>
    <servlet-mapping>
    <servlet-name>finder</servlet-name>
    <url-pattern>/finder</url-pattern>
    </servlet-mapping>
</web-app>
